{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "strictPropertyInitialization": true,
      "skipLibCheck": true,
      "outDir": "./dist",
      "rootDir": ".",
      "baseUrl": ".",
      "paths": {
        "*": ["node_modules/*", "src/types/*"]
      }
    },
    "include": ["src/**/*"]
}